{
  "description": "Adds a page action to toggle the elm debugger.",
  "manifest_version": 2,
  "name": "Elm Debugger Toggle",
  "version": "1.0",
  "homepage_url": "https://github.com/escherlies/elm-debugger-toggle",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": "icons/off.svg",
    "browser_style": true
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "<all_urls>"
  ]
}